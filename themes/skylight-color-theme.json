{
  "name": "skylight",
  "colors": {
    "editor.background": "#161923",
    "editor.foreground": "#A6A28C",
    "sideBar.background": "#22272E",
    "sideBarTitle.foreground": "#ff5c00",
    "statusBar.background": "#101010",
    "breadcrumb.background": "#101010",
    "activityBarBadge.background": "#2f2c3a",
    "activityBar.background": "#101010",
    "activityBarBadge.foreground": "#ff5c00",
    "titleBar.activeBackground": "#000000",
    "statusBarItem.settingsProfilesBackground": "#ff5c00",
    "statusBarItem.settingsProfilesForeground": "#e0dfdb",
  },
  "tokenColors": [
    {
      "name": "Comment, Documentation",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring.multi.python"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6C6C6C"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#A6A28C"
      }
    },
    {
      "name": "Documentation Annotations",
      "scope": [
        "punctuation.definition.block.tag.jsdoc",
        "keyword.other.documentation.javadoc"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#156E7E"
      }
    },
    {
      "name": "Annotations",
      "scope": [
        "storage.type.annotation.java",
        "punctuation.definition.annotation.java"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6C6C6C"
      }
    },
    {
      "name": "Dot, Angle brackets",
      "scope": [
        "punctuation.accessor.js",
        "keyword.operator.relational.js",
        "punctuation.bracket.angle.java",
        "punctuation.separator.period.java",
        "punctuation.separator.period.python",
        "keyword.operator.comparison.python"
      ],
      "settings": {
        "foreground": "#FF7B72"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Constants",
      "scope": [
        "keyword",
        "keyword.other",
        "keyword.control",
        "variable.language",
        "support.constant",
        "constant.escape",
        "constant.numeric",
        "constant.language",
        "storage.modifier",
      ],
      "settings": {
        "foreground": "#F08000"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "meta.tag",
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",

      ],
      "settings": {
        "foreground": "#979DB4"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#F69D50"
      }
    },
    {
      "name": "YAML value",
      "scope": [
        "string.unquoted.plain.out.yaml"
      ],
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#5F7C91"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "meta.block variable.other"
      ],
      "settings": {
        "foreground": "#979DB4"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "storage.type",
        "string.other.link",
        "support.other.variable"
      ],
      "settings": {
        "foreground": "#BDB5D5"
      }
    },
    {
      "name": "Parameters, Lambda Parameters",
      "scope": [
        "variable.parameter",
      ],
      "settings": {
        "foreground": "#87889C"
      }
    },
    {
      "name": "String",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#00A36C"
      }
    },
    {
      "name": "Symbols, Inherited Class, Markup Heading",
      "scope": [
        "constant.other.key",
        "constant.other.symbol",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "meta.use.php",
        "support.other.namespace.php",
        "support.other.namespace.use.php",
        "markup.changed.git_gutter",
        "support.type.sys-types",
        "entity.other.inherited-class",
